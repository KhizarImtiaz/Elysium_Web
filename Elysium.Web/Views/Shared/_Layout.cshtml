<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Stylesheet CSS -->
    <link href="~/css/style.css" rel="stylesheet">

    <title>Elysium</title>
</head>
<body>
    <div class="wrapper">
        <header id="header">
            <nav class="navbar navbar-dark navbar-expand-xl py-2">
                <div class="container position-relative">
                    <button class="navbar-toggler border-0 p-0 shadow-none" type="button"
                            data-bs-toggle="collapse"
                            data-bs-target="#MenuForMobile" aria-controls="MenuForMobile"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <a class="navbar-brand" href="#">
                        <img src="/images/logo.png" alt="Logo" class="img-fluid" />
                    </a>
                    <div class="navbar-collapse collapse">
                        <ul class="navbar-nav align-items-center ms-auto mb-2 mb-lg-0">
                            <li class="nav-item me-2 me-md-4">
                                <a id="Technology" class="nav-link text-uppercase" href="/Home/Index">
                                    TECHNOLOGY
                                </a>
                            </li>
                            <li class="nav-item me-2 me-md-4">
                                <a id="Community" class="nav-link text-uppercase" href="/Home/Community">
                                    COMMUNITY
                                </a>
                            </li>
                            <li class="nav-item me-2 me-md-4">
                                <a id="About" class="nav-link text-uppercase" href="/Home/About">
                                    ABOUT
                                </a>
                            </li>
                            <li class="nav-item me-2 me-md-4">
                                <a id="Developer" class="nav-link text-uppercase" href="/Home/Developer">
                                    DEVELOPER
                                </a>
                            </li>
                            <li class="nav-item me-2 me-md-4">
                                <a id="Blog" class="nav-link text-uppercase" href="/Home/Blog">
                                    BLOG
                                </a>
                            </li>
                            <li class="nav-item me-2 me-md-4">
                                <a id="Contact" class="nav-link text-uppercase" href="/Home/Contact">
                                    CONTACT
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="BtnGreen text-uppercase py-2 px-3 rounded-pill d-inline-block" href="#">
                                    GET STARTED
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
        @RenderBody()
        <footer id="footer" class="Fsize_14 RobotoLight text-white">
            <div class="container">
                <div class="border-top border-bottom py-5">
                    <div class="row pt-5">
                        <div class="col-md-3">
                            <img src="/images/logo-white.png" alt="Logo" class="img-fluid d-block mb-4" />
                            <a class="" target="_blank" href="#"><i class="icon-facebook Fsize_42"></i></a>
                            <a class="" target="_blank" href="#"><i class="icon-twitter Fsize_42"></i></a>
                            <a class="" target="_blank" href="#"><i class="icon-instagram Fsize_42"></i></a>
                            <a class="" target="_blank" href="#"><i class="icon-linkedin Fsize_42"></i></a>
                        </div>
                        <div class="col-md-2 ms-md-auto">
                            <h6 class="mb-4">ELYSIUM</h6>
                            <ul class="list-unstyled mb-0">
                                <li class="py-1"><a href="/Home/About">About</a></li>
                                <li class="py-1"><a href="#">FAQ</a></li>
                                <li class="py-1"><a href="#">Support</a></li>
                                <li class="py-1"><a href="#">Build</a></li>
                                <li class="py-1"><a class="RobotoBold" href="#">Grants and Bounties</a></li>
                                <li class="py-1"><a href="#">Careers</a></li>
                            </ul>
                        </div>
                        <div class="col-md-2 ms-md-auto">
                            <h6 class="mb-4">TECHNOLOGY</h6>
                            <ul class="list-unstyled mb-0">
                                <li class="py-1"><a href="#">Technology</a></li>
                                <li class="py-1"><a href="#">Roadmap</a></li>
                                <li class="py-1"><a href="#">Token</a></li>
                                <li class="py-1"><a href="#">Telemetry</a></li>
                                <li class="py-1"><a class="RobotoBold" href="#">Substrate</a></li>
                                <li class="py-1"><a href="#">Lightpaper</a></li>
                            </ul>
                        </div>
                        <div class="col-md-2 ms-md-auto">
                            <h6 class="mb-4">COMMUNITY</h6>
                            <ul class="list-unstyled mb-0">
                                <li class="py-1"><a href="/Home/Community">Community</a></li>
                                <li class="py-1"><a href="#">Documentation</a></li>
                                <li class="py-1"><a href="#">Brand Assets</a></li>
                                <li class="py-1"><a href="/Home/Blog">Blog</a></li>
                                <li class="py-1"><a class="RobotoBold" href="#">Element Chat</a></li>
                                <li class="py-1"><a href="#">Medium</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="d-flex justify-content-between py-5">
                    © 2022 Elysium
                    <ul class="d-inline-flex list-unstyled mb-0">
                        <li class="me-3"><a href="#">Disclaimer</a></li>
                        <li class="me-3"><a href="#">Privacy</a></li>
                        <li><a href="#">Cookie Settings</a></li>
                    </ul>
                </div>
            </div>
        </footer>

    </div>


    <div class="container">

        @RenderBody()

    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Elysium.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="https://kit.fontawesome.com/4f472b9b00.js" crossorigin="anonymous"></script>
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/custom.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>

<script>

     var currentLocation = window.location.href;
    if (currentLocation.indexOf("Index") >= 0) {
        $("#Technology").addClass('active');
    }
    else if (currentLocation.indexOf("Community") >= 0) {
        $("#Community").addClass('active');
    }
    else if (currentLocation.indexOf("About") >= 0) {
        $("#About").addClass('active');
    }
    else if (currentLocation.indexOf("Developer") >= 0) {
        $("#Developer").addClass('active');
    }
    else if (currentLocation.indexOf("Contact") >= 0) {
        $("#Contact").addClass('active');
    }

</script>